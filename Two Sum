class Solution {
    public int[] twoSum(int[] nums, int target) {
        int n = nums.length;
        for(int i=0;i<n-1;i++){
            for(int j=0;j<n;j++){
                if(nums[i]+nums[j]==target && i!=j){
                    int[] arr= {i,j};
                    return arr;
                }
            }
        }
        return new int[]{};
    }
}
